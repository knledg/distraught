    "knex": "^0.12.5",
    "lodash": ">= 4.7.0",
    "logentries": "^1.0.2",
    "moment": ">= 2.12.0",
    "object-assign": "^4.0.1",
    "pg": "^4.5.1",
    "rollbar": "^0.6.2",
    "sendgrid": "^4.4.0",
    "store": "^1.3.20",
    "vision": "^4.1.0"
