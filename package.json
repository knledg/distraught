    "@jwdotjs/graphql-custom-datetype": "^0.3.1",
    "@jwdotjs/hapi-graphql": "^1.0.6",
    "assert-env": "^0.6.0",
    "axios": ">= 0.14.0",
    "bluebird": "^3.3.1",
    "boom": "^4.1.0",
    "chalk": "^1.1.3",
    "cron": "^1.1.0",
    "es6-error": "^3.1.0",
    "glob": "^7.0.3",
    "good": "^6.6.0",
    "good-console": "^5.3.1",
    "good-logentries": "^2.0.2",
    "graphql": ">= 0.7.1",
    "hapi": "^13.2.1",
    "hapi-auth-cookie": "^6.1.1",
    "hapi-auth-jwt2": "^7.1.3",
    "hapi-swagger": "^5.0.1",
