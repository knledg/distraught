{
  "name": "distraught",
  "version": "0.6.0",
  "description": "An unHapi Node.js server",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run deployjs",
    "deployjs": "babel -i node_modules -d dist src",
    "flow": "node_modules/.bin/flow check",
    "lint": "node_modules/.bin/eslint --ext js .",
    "prepublish": "npm run deploy",
    "preversion": "npm run lint && npm run test && npm run flow",
    "test": "NODE_ENV=test NODE_PATH=. ./node_modules/.bin/mocha test/setup.js test --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knledg/distraught"
  },
  "author": "jwdotjs",
  "license": "MIT",
